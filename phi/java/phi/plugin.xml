<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/phi" factory-class="com.vitessedata.phi.deepgreen.LoadTableNodeFactory" id="com.vitessedata.phi.deepgreen.LoadTableNodeFactory"/>
      <node category-path="/phi" factory-class="com.vitessedata.phi.deepgreen.QueryBuilderNodeFactory" id="com.vitessedata.phi.deepgreen.QueryBuilderNodeFactory"/>
</extension>


   <extension point="org.knime.workbench.repository.categories">
      <category icon="icons/phi.gif" level-id="phi" locked="true" name="VitesseData" path="/">
      </category>
   </extension>
   
</plugin>
